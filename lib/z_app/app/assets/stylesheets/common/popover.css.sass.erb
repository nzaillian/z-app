@import 'common/common'

.popover-form-wrap
  width: 240px
  padding: 6px 6px 6px 6px
  
  .left-side, .right-side
    display: block
    float: left

  .left-side
    width: 80px
    text-align: right
    font-size: 14px
    padding-top: 2px

  .right-side
    white-space: nowrap
    float: right
    width: 128px

  .left-side.left-align, .right-side.left-align        
    text-align: left

  .breaker
    clear: both
    height: 10px